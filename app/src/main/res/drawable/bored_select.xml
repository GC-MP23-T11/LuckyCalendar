<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 체크된 상태일 때 표시할 이미지 (테두리가 있는 이미지) -->
    <item android:drawable="@drawable/bored_icon_border" android:state_checked="true"/>
    <!-- 체크되지 않은 상태일 때 표시할 이미지 (테두리가 없는 이미지) -->
    <item android:drawable="@drawable/bored_icon"/>
</selector>
